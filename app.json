{
  "expo": {
    "name": "Uppi",
    "slug": "uppi",
    "description": "Uppi is a tool to monitor your websites and servers, and get notified when something is wrong.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "uppi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2625"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.uppi.app",
      "usesNonExemptEncryption": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#DC2625"
      },
      "androidNavigationBar": {
        "backgroundColor": "#DC2625"
      },
      "package": "dev.uppi.app"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc795f47-a09e-4843-8631-6883d5b361c8"
      }
    },
    "owner": "webmethod"
  }
}
